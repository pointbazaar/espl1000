#include <stdio.h>

fn main () ~> int {
	
	//A Harshad Number is a number that is divisible
	//by the sum of its digits

	for i in 1 .. 20 {
		
		testharshad(i);
		printf("\n");
	}
	
	return 0;
}

fn testharshad (int n) ~> int {

	norig = n;
	sum = 0;
	
	while n != 0 {

		sum = sum + n % 10;
		n = n / 10;
	}

	bool res = norig % sum == 0;

	if res { 
		printf("%d-is-harshad", norig);
	} else {
		printf("%d-is-not-harshad", norig);
	}


	return 0;
}

