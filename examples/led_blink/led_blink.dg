
fn main () ~> int {
	//set D13 / PB5

	//set PORTB Data Direction Register
	[0x24] = 0b00100000;
	
	while(true){
		//set PORTB Data Register
		[0x25] = 0b00100000;
		
		//wait
		delay();
		
		[0x25] = 0b00000000;
		
		//wait
		delay();
	}
	return 1;
}

