fn test_collision2d() ~> int throws {

	Collider2D c1 = collider2d_ctor(
		v2_ctor(0.0,0.0),
		v2_ctor(2.0,2.0)
	);
	
	Collider2D c2 = collider2d_ctor(
		v2_ctor(1.0,1.0),
		v2_ctor(2.0,2.0)
	);
	
	Collider2D c3 = collider2d_ctor(
		v2_ctor(4.0,1.0),
		v2_ctor(2.0,2.0)
	);
	
	assert(collision2d_overlap(c1,c2));
	
	assert(!collision2d_overlap(c1,c3));
	
	return 0;
}
